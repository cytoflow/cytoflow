<?xml version="1.0" encoding="UTF-8"?>

<comp:Compensation-ML 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:comp="http://www.isac-net.org/std/Compensation-ML/v1.0/"
    xsi:schemaLocation="http://www.isac-net.org/std/Compensation-ML/v1.0/ ../schema/Compensation-ML.v1.0.xsd">
    
    <!-- 
        The amount of Fluor signal in the PhyEry detector for a Fluor-stained cell is 0.16501 (16.501 %).
        The amount of Fluor signal in the Cy5PE detector for a Fluor-stained cell is 0.03457 (3.457 %).
        
        The amount of PhyEry signal in the Fluor detector for a PhyEry-stained cell is 0.01454 (1.454 %).
        The amount of PhyEry signal in the Cy5PE detector for a PhyEry-stained cell is 0.21343 (21.343 %).
        
        The amount of Cy5PE signal in the Fluor detector for a Cy5PE-stained cell is 0.0059 (0.59 %).
        The amount of Cy5PE signal in the PhyEry detector for a Cy5PE-stained cell is 0.23623 (23.623 %).
    -->
    
    <comp:spilloverMatrix comp:id="Fluor-PhyEry-Cy5PE-Compensation">
      
        <comp:spillover comp:parameter="Fluor">
            <comp:coefficient comp:parameter="PhyEry" comp:value="0.16501"/>
            <comp:coefficient comp:parameter="Cy5PE" comp:value="0.03457"/>
        </comp:spillover>
 
        <comp:spillover comp:parameter="PhyEry">
            <comp:coefficient comp:parameter="Fluor" comp:value="0.01454"/>
            <comp:coefficient comp:parameter="Cy5PE" comp:value="0.21343"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="Cy5PE">
            <comp:coefficient comp:parameter="Fluor" comp:value="0.0059"/>
            <comp:coefficient comp:parameter="PhyEry" comp:value="0.23623"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    
    
    <!-- 
        The amount of FITC signal in the PE detector for a FITC-stained cell is 0.15 (15 %).
        The amount of PE signal in the FITC detector for a PE-stained cell is 0.01 (1 %).
    -->
    
    <comp:spilloverMatrix comp:id="FITC-PE-Compensation">
        
        <comp:spillover comp:parameter="FITC">
            <comp:coefficient comp:parameter="FITC" comp:value="1"/>
            <comp:coefficient comp:parameter="PE" comp:value="0.15"/>
        </comp:spillover>
        
        <comp:spillover comp:parameter="PE">
            <comp:coefficient comp:parameter="FITC" comp:value="0.01"/>
            <comp:coefficient comp:parameter="PE" comp:value="1"/>
        </comp:spillover>
        
    </comp:spilloverMatrix>
    
    
</comp:Compensation-ML>
    
    